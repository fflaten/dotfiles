{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "background": "#1d1d1d",
    "foreground": "black",
    "lightText": "white"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "austin",
            "threshold": 500
          },
          "template": " <p:background>\ufbab</> {{ .FormattedMs }} ",
          "foreground": "p:foreground",
          "powerline_symbol": "\ue0b0",
          "background": "lightRed",
          "type": "executiontime",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf0e7 ",
          "foreground": "p:foreground",
          "powerline_symbol": "\ue0b0",
          "background": "yellow",
          "type": "root",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if .Error }}‚ùå{{ end }}",
          "type": "status",
          "style": "plain"
        },
        {
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "cache_duration": "none",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31c",
            "windows": "\ue70f",
            "wsl": "\ue712",
            "wsl_separator": " on "
          },
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "foreground": "p:lightText",
          "powerline_symbol": "\ue0b0",
          "background": "darkGray",
          "type": "os",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if .SSHSession }} \uf817 {{ .UserName }}@{{ .HostName }}<darkGray> \ue0b1</>{{ end }}",
          "foreground": "p:lightText",
          "powerline_symbol": "\ue0b0",
          "background": "p:background",
          "type": "session",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "üìÇ",
            "hide_root_location": false,
            "home_icon": "üè†",
            "mapped_locations": {
              "~/Documents": "üìÑ",
              "~/Git": "üöß",
              "~/OneDrive": " <blue>\uf8c9 </>"
            },
            "mapped_locations_enabled": true,
            "style": "short"
          },
          "template": "{{ .Path }}",
          "foreground": "cyan",
          "powerline_symbol": "\ue0b0",
          "background": "p:background",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "track_separator": " - "
          },
          "template": "<darkGray>\ue0b1 </>{{ .Icon }}{{ if ne .Status \"stopped\" }}{{ .Artist }} - {{ .Track }}{{ end }}",
          "foreground": "lightGreen",
          "powerline_symbol": "\ue0b0",
          "background": "p:background",
          "type": "spotify",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{if ne .Env.TERM_PROGRAM \"vscode\"}}<darkGray>\ue0b1 </>{{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}} \uf692 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }} {{ end }}",
          "foreground": "lightYellow",
          "powerline_symbol": "\ue0b0",
          "background": "p:background",
          "type": "git",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "foreground": "p:foreground",
          "powerline_symbol": "\ue0b0",
          "background": "magenta",
          "type": "python",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if .Full }}<darkGray>\ue0b1</> \ue77f {{ .Full }} {{ end }}",
          "powerline_symbol": "\ue0b0",
          "background": "p:background",
          "type": "dotnet",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "alias": "Time",
          "template": "\uf43a {{ .CurrentDate | date .Format }}",
          "foreground": "p:lightText",
          "type": "time",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "‚ùØ",
          "foreground": "lightGreen",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
